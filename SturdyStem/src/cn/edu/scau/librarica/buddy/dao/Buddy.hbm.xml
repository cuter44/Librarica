<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.edu.scau.librarica.buddy.dao.Buddy" table="buddy_buddy">
        <id	name="id" type="long">
            <generator class="identity" />
        </id>

        <property name="r" type="byte" />
        <many-to-one name="me" class="cn.edu.scau.librarica.authorize.dao.User" />
        <many-to-one name="op" class="cn.edu.scau.librarica.authorize.dao.User" />
    </class>
</hibernate-mapping>