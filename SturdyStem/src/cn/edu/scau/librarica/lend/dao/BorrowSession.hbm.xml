<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.edu.scau.librarica.lend.dao">
    <class name="BorrowableBook" table="lend_borrowable_book">
        <id	name="id" type="long" column="id">
            <generator class="identity" />
        </id>

        <property name="status" type="byte" />
        <one-to-one name="book" class="cn.edu.scau.librarica.shelf.dao.Book" />
        <one-to-one name="borrower" class="cn.edus.librarica.authorize.dao.User" />
    </class>
</hibernate-mapping>